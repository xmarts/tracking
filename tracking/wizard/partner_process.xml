<?xml version="1.0" encoding="utf-8"?>
<odoo>
        <record id="view_res_partner_process" model="ir.ui.view">
            <field name="name">Procesar</field>
            <field name="model">res.partner.process</field>
            <field name="arch" type="xml">
                <form string="Procesar">
                    <p class="oe_grey">
                        Procesar rutas
                    </p>
                    <group>
                        <field name="count" invisible="[('count','=',1)]" readonly="True"/>
                        <!-- <field name="dias"/> -->
                        <group>
                            <field name="type"/>
                            <field name="zona"/>
                            <field name="entry_date"/>
                        </group>
                        <group>
                            <field name="inicio"/>
                            <field name="fecha_fin"/>
                        </group>
                    </group>
                    <footer>
                        <button name="process" string="Procesar" type="object"
                            class="btn-primary"/>
                        <button name="proccess_and_updated" string="Procesar/Actualizar" type="object"
                        class="btn-primary"/>
                        <button name="process2" string="Procesar Lacteos" type="object"
                            class="btn-primary"/>
                        <button string="Cancel" class="btn-default" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>

        <act_window name="Procesar"
            res_model="res.partner.process"
            src_model="res.partner"
            key2="client_action_multi"
            multi="True"
            view_mode="form"
            view_type="form"
            target="new"
            id="action_res_partner_process"/>

      <!-- Wizard sequence -->

        <record model="ir.ui.view" id="sequence_wizard_wizard">
            <field name="name">quotations.report.wizard</field>
            <field name="model">sequence.assign.wizard</field>
            <field name="arch" type="xml">
                <form>
                    <group>
                        <group>
                            <field name="dias"/>
                        </group>
                        <group>
                        </group>
                    </group>
                    <footer>
                        <button name="assign" string="Asignar" type="object"
                        class="btn-primary"/>
                        <button string="Cancel" class="btn-default" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>
</odoo>